
import java.util.Scanner;

public class ders10 {

	public static void main(String[] args) {
		sayiSay say1 = new sayiSay();
		int sayiA,sayiB,sayiC,sayiD;
		Scanner sc = new Scanner(System.in);
		/*
		System.out.println("Başlangıç değerini giriniz	:	");
		sayiA=sc.nextInt();

		System.out.println("Bitiş değerini giriniz	:	");
		sayiB=sc.nextInt();

		say1.sayiSay1(sayiA,sayiB);
		*/
		System.out.println("Başlangıç değerini giriniz	:	");
		sayiC=sc.nextInt();

		System.out.println("Bitiş değerini giriniz	:	");
		sayiD=sc.nextInt();
		
		say1.sayiSay2(sayiC,sayiD);
	}
}



public class sayiSay {
	
	public void sayiSay1(int baslangic, int bitis)
		{
		int sayi=baslangic;
		while(sayi<bitis)
			{
			System.out.println(sayi);
			sayi=sayi+1;
			}
		}
	public void sayiSay2(int baslangic, int bitis)
	{
	int sayi=baslangic;
	while(sayi<bitis)
		{
		System.out.println(sayi);
		sayi=sayi+2;
		}
	}
	}
