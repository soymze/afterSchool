
import java.util.Scanner;

public class ders15 {

	public static void main(String[] args) {
		
		int[] a = new int[10];
		int i=0;
		int sonuc=0;
		
		Scanner sc= new Scanner(System.in);
		
		while(i<5) {
			System.out.print((i+1) + ". sayıyı giriniz : " );
			a[i]=sc.nextInt();
			i++;
		}
		
		for(int x=0;x<5;x++)
		{
			sonuc+=a[x];
		}
		System.err.println("Sonuç =	"	+sonuc);
	}

}
